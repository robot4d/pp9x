,"Mob-7013001":{ "appearance" : "02guihune", "attackNonsense" : [ "是不是你害得我不得超生？", "我是不灭的！", "天黑了，你跑不掉的。" ], "attackType" : "mix", "creature" : "undead", "defaultSpellId" : "SpellTemplate-521", "description" : "游荡在白骨洞内的鬼魂，早已忘记前生之事，无法堕入轮回。", "gender" : "male", "maxLevel" : 125, "minLevel" : 20, "monsterId" : "Mob-7013001", "monsterType" : "normal", "name" : "狂暴游魂", "nonsense" : [ "不做亏心事，不怕我叫门。", "我的头呢？谁看见了我的头？", "为什么这一切还没结束啊。。。。。。" ] }
,"Mob-7013002":{ "appearance" : "02kulouc", "attackNonsense" : [ "我问你，说实话，我瘦么？" ], "attackType" : "mix", "creature" : "undead", "defaultSpellId" : "SpellTemplate-525", "description" : "乱葬岗中埋葬的无名枯骨，生前战死的他们被乱葬岗的阴气感染，成为只有战斗本能的白骨战士。", "gender" : "male", "maxLevel" : 125, "minLevel" : 20, "monsterId" : "Mob-7013002", "monsterType" : "normal", "name" : "狂暴骨卒", "nonsense" : [ "我曾经和你一样，总有一天。。。。" ] }
,"Mob-7013003":{ "appearance" : "02kuloud", "attackNonsense" : [ "我曾经和你一样，总有一天……", "白费力气，我是不会死的！", "天哪，我骨折了！" ], "attackType" : "mix", "creature" : "undead", "defaultSpellId" : "SpellTemplate-525", "description" : "天尸白晶晶的仆从，平日虽然做着端茶倒水的工作，却是绝强的武者。他们心中只有一个信念：无论谁想要接近女王，都将死在他们的刀剑下。\n", "gender" : "male", "maxLevel" : 125, "minLevel" : 20, "monsterId" : "Mob-7013003", "monsterType" : "normal", "name" : "狂暴骨仆", "nonsense" : [ "有钱难买老来瘦。", "我的腿！我的腿!快抓住那只狗！", "为什么喝酒会露出来呢？" ] }
,"Mob-7013004":{ "appearance" : "12niutouren", "attackNonsense" : [ "说，你的内衣是不是红色的！", "虽然看不到，但我还是要发怒了！" ], "attackType" : "mix", "creature" : "yao", "defaultSpellId" : "SpellTemplate-530", "description" : "一般的牛妖，因身体强壮，力大超群而成为虎妖的主力爪牙。\n", "gender" : "male", "maxLevel" : 125, "minLevel" : 20, "monsterId" : "Mob-7013004", "monsterType" : "normal", "name" : "狂暴牛鬼", "nonsense" : [ "作为一只老牛，我最喜欢吃嫩草了。", "什么？你说我下流？其实是你想多了。" ] }
,"Mob-7013005":{ "appearance" : "02sheyaoa", "attackNonsense" : [ "你打到我的蛀牙了，太疼了！", "我要毒死你！你这个坏牙医！" ], "attackType" : "mix", "creature" : "yao", "defaultSpellId" : "SpellTemplate-532", "description" : "久居山中的蛇怪，因道行不足而无法完全化为人形。主要以毒牙伤人。\n", "gender" : "male", "maxLevel" : 125, "minLevel" : 20, "monsterId" : "Mob-7013005", "monsterType" : "normal", "name" : "狂暴蛇神", "nonsense" : [ "最近牙很疼，我是不是该看牙医了？", "可是敢给我拔牙的医生太少了……" ] }
,"Mob-7013006":{ "appearance" : "12huoyantongzi", "attackNonsense" : [ "太好了，晚餐的材料自己送上门来了。", "看你的样子应该没有注水。" ], "attackType" : "mix", "creature" : "yao", "defaultSpellId" : "SpellTemplate-536", "description" : "虎力大仙手下的小妖，因深得宠爱而被传授了灵力，化作童子模样。", "gender" : "male", "maxLevel" : 125, "minLevel" : 20, "monsterId" : "Mob-7013006", "monsterType" : "champion", "name" : "狂暴童子", "nonsense" : [ "快点给虎仙大人准备晚饭吧。", "不然它一生气，会把我们都吃了的。" ] }
,"Mob-7013007":{ "appearance" : "02yuanling", "attackNonsense" : [ "你别想伤害我！", "别过来！别过来！" ], "attackType" : "mix", "creature" : "yao", "defaultSpellId" : "SpellTemplate-537", "description" : "被囚禁与塔底的女妖，因为与世隔绝多年而逐渐陷入疯狂，以致她们会攻击见到的每一个人。", "gender" : "female", "maxLevel" : 125, "minLevel" : 20, "monsterId" : "Mob-7013007", "monsterType" : "normal", "name" : "狂暴女妖", "nonsense" : [ "这是哪里？我是谁？", "这里太黑了，我害怕！" ] }
,"Mob-7013008":{ "appearance" : "09heiwuchang", "attackNonsense" : [ "我要挖出你的眼珠。", "有了你的眼珠，我就能复明了！" ], "attackType" : "mix", "creature" : "yao", "defaultSpellId" : "SpellTemplate-521", "description" : "负责在塔底布施结界的夜灵，因为久居黑暗而失去视力，所以会攻击每一个接近的人。\n", "gender" : "male", "maxLevel" : 125, "minLevel" : 20, "monsterId" : "Mob-7013008", "monsterType" : "normal", "name" : "狂暴夜灵", "nonsense" : [ "我看不见，但我能听见一切。", "我听到有活人的心跳声了。" ] }
,"Mob-7013009":{ "appearance" : "13zuiyexingzhea", "attackNonsense" : [ "让火焰融化你吧！", "和我一起来被炙烤吧！" ], "attackType" : "mix", "creature" : "yao", "defaultSpellId" : "SpellTemplate-522", "description" : "背负人间罪恶的灵体，周身业火烧灼，不得安宁。\n", "gender" : "male", "maxLevel" : 125, "minLevel" : 20, "monsterId" : "Mob-7013009", "monsterType" : "champion", "name" : "狂暴行者", "nonsense" : [ "疼啊！疼啊！什么时候才能结束啊！", "这些火焰何事才能熄灭？！" ] }
,"Mob-7013010":{ "appearance" : "10langyao", "attackNonsense" : [ "你再厉害也不是狼群的对手。", "我是狼，不是狗，不玩捡骨头。" ], "attackType" : "mix", "creature" : "yao", "defaultSpellId" : "SpellTemplate-542", "description" : "狮魔王手下的妖化狼人，主要驻扎在狮驼岭的咽喉之处，平日里负责猎杀过往的行人与周围的动物。\n", "gender" : "male", "maxLevel" : 125, "minLevel" : 20, "monsterId" : "Mob-7013010", "monsterType" : "normal", "name" : "狂暴狼妖", "nonsense" : [ "狮驼岭周围方圆百里所有人都是我们的食物。", "你一脚踏进狮驼岭就等于踏上我们的餐桌了。" ] }
,"Mob-7013011":{ "appearance" : "13huxianshizhe", "attackNonsense" : [ "我脑门上的王字可不是白写的！", "小心被我咬断喉咙。" ], "attackType" : "mix", "creature" : "yao", "defaultSpellId" : "SpellTemplate-521", "description" : "狮魔王的近卫军，平时驻扎在狮魔王营寨的周围的虎妖精锐部队。\n", "gender" : "male", "maxLevel" : 125, "minLevel" : 20, "monsterId" : "Mob-7013011", "monsterType" : "normal", "name" : "狂暴虎人", "nonsense" : [ "养兵千日，也不见用兵一时。", "无聊的时候，我们喜欢数星星。" ] }
,"Mob-7013012":{ "appearance" : "13shiren", "attackNonsense" : [ "你也不要看扁了我们。", "以前我们也是在非洲混过的！" ], "attackType" : "mix", "creature" : "yao", "defaultSpellId" : "SpellTemplate-531", "description" : "狮魔王的贴身护卫，都是狮魔王的亲信，平时不离狮魔王左右。如果你看到了它们，证明你已经离狮魔王不远了。\n", "gender" : "male", "maxLevel" : 125, "minLevel" : 20, "monsterId" : "Mob-7013012", "monsterType" : "champion", "name" : "狂暴狮王", "nonsense" : [ "其实我们是大王的亲戚。", "我们是靠驼关系来混口饭吃的。" ] }
,"Mob-7013013":{ "appearance" : "04zhongyuanshibingb", "attackNonsense" : [ "我们不敢了！再也不敢扰民了！", "我马上放下武器投降，别再打我了。" ], "attackType" : "mix", "creature" : "human", "defaultSpellId" : "SpellTemplate-525", "description" : "虎威军中的低阶士兵，通常是由强拉来的新兵组成。", "gender" : "male", "maxLevel" : 125, "minLevel" : 20, "monsterId" : "Mob-7013013", "monsterType" : "normal", "name" : "狂暴军士", "nonsense" : [ "这皇粮吃得半饥半饱啊。", "还没到发饷的日子吗？" ] }
,"Mob-7013014":{ "appearance" : "04zhongyuanjiangjun", "attackNonsense" : [ "我要用旋风撕裂你身体！", "尝尝我的风之刃吧！", "我……尽力了，你很强。" ], "attackType" : "mix", "creature" : "demon", "defaultSpellId" : "SpellTemplate-526", "description" : "古时商超纣王下属的猛将，他的部队以进军神速、出其不意而著称。其死后阴魂不散，率领旧部阴兵试图复辟商纣王朝。\n", "gender" : "male", "maxLevel" : 125, "minLevel" : 20, "monsterId" : "Mob-7013014", "monsterType" : "champion", "name" : "狂暴魔将", "nonsense" : [ "我飞廉就是风的化身。", "我可日行千里！来去如风。", "反我商王者死！" ] }
