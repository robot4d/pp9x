,"Mob-0005001":{ "appearance" : "05huayao", "attackNonsense" : [ "我要用你的鲜血来染红我的花冠！", "成为我脚下的养料吧！" ], "attackType" : "mix", "creature" : "yao", "defaultSpellId" : "SpellTemplate-505", "description" : "本来是天香教弟子所栽培的普通花草，后因为感染了不明孢毒而妖化，并陷入了疯狂状态。\n", "gender" : "female", "maxLevel" : 60, "minLevel" : 60, "monsterId" : "Mob-0005001", "monsterType" : "normal", "name" : "花妖", "nonsense" : [ "花需要绽放，所以我们需要更多的血来浇灌。", "如果要延长花期的话，我们需要更多的骨肉来施肥。" ] }
