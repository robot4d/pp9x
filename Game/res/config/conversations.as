,"con-103004":{ "conversationId" : "con-103004", "dialogues" : [ { "no" : 1, "content" : "工头洪伍：演武堂破了个洞？一定是你们大师兄干的吧？真不知道你们长居深海之中，怎么脾气还那么大。我手头工作完事就过去。", "condition" : {}, "action" : {}, "name" : "", "portrait" : "", "optionLists" : [ { "order" : 1, "no" : 2, "isLink" : false } ] } ], "name" : "师门任务", "options" : [ { "no" : 0, "content" : "对话树根", "condition" : [], "action" : [], "dialogueLists" : [ { "order" : 1, "no" : 1, "isLink" : false } ] }, { "no" : 2, "content" : "那好，我知道了，我这就去。", "condition" : {}, "action" : {}, "dialogueLists" : [] } ], "type" : "normal" }
