,"conv-405007":{ "conversationId" : "conv-405007", "dialogues" : [ { "no" : 1, "content" : "四王爷刘渊：太上道诸位仙长平日里经常炼制仙丹，所以药材消耗很大。我这里正好购进了一批珍贵药材，请你跑趟太阿，将这些珍奇草药送给张伯端吧。", "condition" : { "category" : "questCanAccept", "params" : [ "" ] }, "action" : [], "name" : "", "portrait" : "", "optionLists" : [ { "order" : 1, "no" : 2, "isLink" : false } ] }, { "no" : 3, "content" : "张伯端：你送来的这些都是上等药材，我们一定会善加利用的。", "condition" : { "category" : "questCanSubmit", "params" : [ "" ] }, "action" : [], "name" : "", "portrait" : "", "optionLists" : [ { "order" : 1, "no" : 4, "isLink" : false } ] } ], "name" : "千里行镖", "options" : [ { "no" : 0, "content" : "对话树根", "condition" : [], "action" : [], "dialogueLists" : [ { "order" : 1, "no" : 1, "isLink" : false }, { "order" : 2, "no" : 3, "isLink" : false } ] }, { "no" : 2, "content" : "好的，我马上启程。", "condition" : [], "action" : { "category" : "acceptQuest", "params" : [ "" ] }, "dialogueLists" : [] }, { "no" : 4, "content" : "希望这些药材可以对大家有所助益。", "condition" : [], "action" : { "category" : "submitQuest", "params" : [ "" ] }, "dialogueLists" : [] } ], "type" : "quest" }
