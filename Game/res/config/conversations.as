,"conv-004047":{ "conversationId" : "conv-004047", "dialogues" : [ { "no" : 1, "content" : "薛承制：第一场比试，是你对我的师弟陆承风。现在你可以上台去和他比试了。", "condition" : { "category" : "questCanAccept", "params" : [ "" ] }, "action" : {}, "name" : "", "portrait" : "", "optionLists" : [ { "order" : 1, "no" : 2, "isLink" : false } ] }, { "no" : 3, "content" : "陆承风：承风掌！陆家剑！无敌表妹摄魂图！看来你运气不好，碰到我算你倒霉。", "condition" : { "category" : "questCanSubmit", "params" : [ "" ] }, "action" : [], "name" : "", "portrait" : "", "optionLists" : [ { "order" : 1, "no" : 4, "isLink" : false } ] } ], "name" : "擂台比试（二）", "options" : [ { "no" : 0, "content" : "对话树根", "condition" : [], "action" : [], "dialogueLists" : [ { "order" : 1, "no" : 1, "isLink" : false }, { "order" : 2, "no" : 3, "isLink" : false } ] }, { "no" : 2, "content" : "是他……我正想收拾一下他呢。", "condition" : [], "action" : { "category" : "acceptQuest", "params" : [ "" ] }, "dialogueLists" : [] }, { "no" : 4, "content" : "来PK吧，小陆陆。", "condition" : {}, "action" : { "category" : "submitQuest", "params" : [ "" ] }, "dialogueLists" : [] } ], "type" : "quest" }
