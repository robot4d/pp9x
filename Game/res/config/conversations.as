,"conv-706001":{ "conversationId" : "conv-706001", "dialogues" : [ { "no" : 1, "content" : "敖怜珠：黑鲨帮攻打我们已久，我不但不能帮师弟们抵抗入侵，还连累他们保护我，实在于心难安。[PC_GENDER:4]，我愿用种植多年的紫丹酬劳，你能否帮我将黑鲨帮主打伤，缓解师弟们的压力?", "condition" : { "category" : "questCanAccept", "params" : [ "" ] }, "action" : {}, "name" : "", "portrait" : "", "optionLists" : [ { "order" : 1, "no" : 2, "isLink" : false } ] }, { "no" : 3, "content" : "敖怜珠：黒鲨帮的攻势果然小了很多，真是多谢你了，这些紫丹不成敬意。", "condition" : { "category" : "questCanSubmit", "params" : [ "" ] }, "action" : [], "name" : "", "portrait" : "", "optionLists" : [ { "order" : 1, "no" : 4, "isLink" : false } ] } ], "name" : "收集紫丹", "options" : [ { "no" : 0, "content" : "对话树根", "condition" : [], "action" : [], "dialogueLists" : [ { "order" : 1, "no" : 1, "isLink" : false }, { "order" : 2, "no" : 3, "isLink" : false } ] }, { "no" : 2, "content" : "包在我身上。", "condition" : [], "action" : { "category" : "acceptQuest", "params" : [ "" ] }, "dialogueLists" : [] }, { "no" : 4, "content" : "这是我应该做的。", "condition" : [], "action" : { "category" : "submitQuest", "params" : [ "" ] }, "dialogueLists" : [] } ], "type" : "quest" }
