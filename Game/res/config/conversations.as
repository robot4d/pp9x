,"conv-405001":{ "conversationId" : "conv-405001", "dialogues" : [ { "no" : 1, "content" : "四王爷刘渊：最近乱象环生，搞得天下民不聊生。百姓疾苦我等岂能视而不见。我这里筹措了一批赈灾的银两和物资，请你押运至白云镇交给镇长蔡丰田吧。路上请千万小心，这些物资可都是灾民的命根啊。", "condition" : { "category" : "questCanAccept", "params" : [ "" ] }, "action" : [], "name" : "", "portrait" : "", "optionLists" : [ { "order" : 1, "no" : 2, "isLink" : false } ] }, { "no" : 3, "content" : "蔡丰田：这真是雪中送炭，我这里正为这些难民的生计发愁呢。", "condition" : { "category" : "questCanSubmit", "params" : [ "" ] }, "action" : [], "name" : "", "portrait" : "", "optionLists" : [ { "order" : 1, "no" : 4, "isLink" : false } ] } ], "name" : "千里行镖", "options" : [ { "no" : 0, "content" : "对话树根", "condition" : [], "action" : [], "dialogueLists" : [ { "order" : 1, "no" : 1, "isLink" : false }, { "order" : 2, "no" : 3, "isLink" : false } ] }, { "no" : 2, "content" : "救民于水火，在下义不容辞。", "condition" : [], "action" : { "category" : "acceptQuest", "params" : [ "" ] }, "dialogueLists" : [] }, { "no" : 4, "content" : "能亲手把赈灾钱粮转送到您手里，我也安心了。", "condition" : [], "action" : { "category" : "submitQuest", "params" : [ "" ] }, "dialogueLists" : [] } ], "type" : "quest" }
